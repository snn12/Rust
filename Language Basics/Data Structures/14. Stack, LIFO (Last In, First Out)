fn main() {
    let mut stack: Vec<i32> = Vec::new();

    // Elementləri əlavə edirik
    stack.push(1);
    stack.push(2);
    stack.push(3);

    println!("Stack: {:?}", stack); // Çıxış: Stack: [1, 2, 3]

    // Sonuncu elementi çıxarırıq
    let top = stack.pop();
    println!("Top of stack: {:?}", top); // Çıxış: Top of stack: Some(3)
}
